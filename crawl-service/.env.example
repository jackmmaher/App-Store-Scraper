# Crawl4AI Service Configuration

# API Security
CRAWL_SERVICE_API_KEY=your-secret-api-key-here

# Supabase (for caching crawled content)
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_SERVICE_KEY=eyJ...

# Rate Limits
CRAWL_MAX_CONCURRENT=5
CRAWL_REQUESTS_PER_MINUTE=30
CRAWL_CACHE_TTL_HOURS=24

# Crawl4AI Settings
CRAWL4AI_HEADLESS=true
CRAWL4AI_BROWSER=chromium

# Logging
LOG_LEVEL=INFO
